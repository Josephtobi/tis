        <h1 class="register-sign"> Conference <span class="h1r"> registration</span></h1>
        <hr class="container">

        <form action="/register-c" method="POST" class="container">
            <div class="row">
                <div class="form-group col-6">
                    <label for="F-name-c">First Name</label>
                    <input name="FirstName" type="text" class="form-control" id="F-name-c" aria-describedby="F-name-c" placeholder="Enter your first name" required>
                </div>

                <div class="form-group col-6">
                    <label for="L-name-c">Last name </label>
                    <input name="LastName" type="text" class="form-control" id="L-name-c" aria-describedby="L-name-c" placeholder="Enter your last name" required>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-7">
                    <label for="Email-c">Email address</label>
                    <input name="Email" type="email" class="form-control" id="Email-c" aria-describedby="email" placeholder="Enter a valid Email address" required>
                </div>
                <div class="form-group col-5">
                    <label for="tel-c">Phone number</label>
                    <input name="Phone" type="tel" class="form-control" id="tel-c" aria-describedby="tel-c" placeholder="Enter a valid phone number" required>
                </div>

            </div>
            <div class="row">
                <div class="form-group col-6">
                    <label for="inputSchool">School</label>
                    <select name="School" id="inputSchool" class="form-control">
                      <option value="University of Lagos">University of Lagos</option>
                      <option value="University of Ibadan">University of Ibadan</option>
                      <option value="Convenant University">Convenant University</option>
                      <option value="Babcock University">Babcock University</option>
                      <option value="Pan-Atlantic University">Pan-Atlantic University</option>
                      <option value="Obafemi Awolowo University">Obafemi Awolowo University</option>
                      <option value="Lagos state University">Lagos state University</option>
                      <option value="Federal College of Education">Federal College of Education</option>
                      <option value="Caleb University">Caleb University</option>
                      <option value="other..." >other...</option>
                    </select>
                  </div>

                  <div class="form-group col-4">
                    <label for="inputDept">Department</label>
                    <select name="Department" id="inputDept" class="form-control">
                      <option value="Economics">Economics</option>
                      <option value="Accounting">Accounting</option>
                      <option value="Political Science">Political Science</option>
                      <option value="Sociology">Sociology</option>
                      <option value="Law">Law</option>
                      <option value="Mass Communication">Mass Communication</option>
                      <option value="Creative Arts">Creative Arts</option>
                      <option value="Theatre Arts">Theatre Arts</option>
                      <option value="Business Administration">Business Administration</option>
                      <option value="other">other...</option>
                    </select>
                  </div>

                  <div class="form-group col-2">
                    <label for="inputLevel">Level</label>
                    <select name="Level" id="inputLevel" class="form-control">
                      <option value="100">100</option>
                      <option value="200">200</option>
                      <option value="300">300</option>
                      <option value="400">400</option>
                    </select>
                  </div>
                
            </div>
            <div class="btn-container">
                <button type="submit" class="btn">Register</button>
            </div>
        </form>


